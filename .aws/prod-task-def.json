{
  "family": "moonveil-site-backend-prod",
  "containerDefinitions": [
    {
      "name": "moonveil-site-backend-prod",
      "image": "597802188410.dkr.ecr.ap-southeast-1.amazonaws.com/moonveil-site-backend-prod",
      "portMappings": [
        {
          "name": "moonveil-site-backend-prod-3000-tcp",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "AWS_PARAM_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:597802188410:parameter/param/access/aws/key-id"
        },
        {
          "name": "AWS_PARAM_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:ssm:ap-southeast-1:597802188410:parameter/param/access/aws/secret-key"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/moonveil-site-backend-prod",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::597802188410:role/EcsTaskRole",
  "executionRoleArn": "arn:aws:iam::597802188410:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "4096",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}